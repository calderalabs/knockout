<header class="ko-player__header">
  <h1 class="ko-player__title">
    {{#tool-tipster title=title}}
      {{teamOneFullName}} vs {{teamTwoFullName}} game {{matchNumber}}
    {{/tool-tipster}}
  </h1>

  <div class="ko-player__close" onclick={{action "close"}}><a>Close</a></div>
</header>

<div class="ko-player__vod">
  {{component vodComponentName vod=vod}}
</div>
